{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@src": [
        "./"
      ],
      "@src/*": [
        "./*"
      ],
      "@apis": [
        "./apis"
      ],
      "@apis/*": [
        "./apis/*"
      ],
      "@assets": [
        "./assets"
      ],
      "@assets/*": [
        "./assets/*"
